{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^2.3",
        "drupal/blazy": "^2.0.0-rc3",
        "drupal/bootstrap4": "^2.1",
        "drupal/color_field": "2.*",
        "drupal/colorbox": "1.x-dev",
        "drupal/colorbutton": "^1.0",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/cshs": "^2.3",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "^1.2",
        "drupal/entity": "^1.2",
        "drupal/entity_reference_revisions": "^1.2",
        "drupal/field_group": "^3.1",
        "drupal/file_mdm": "^2.1",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/font_awesome": "^1.4",
        "drupal/fz152": "^1.2",
        "drupal/image_effects": "^3.0",
        "drupal/linkit": "^6.0",
        "drupal/metatag": "^1.14",
        "drupal/paragraphs": "^1.1",
        "drupal/paragraphs_browser": "^1.0",
        "drupal/pathauto": "^1.0@RC",
        "drupal/redirect": "^1.4",
        "drupal/simple_gmap": "^3.0",
        "drupal/simple_sitemap": "^3.7",
        "drupal/slick": "^2.0.0-rc3",
        "drupal/slick_paragraphs": "^2.0",
        "drupal/svg_image": "^1.14",
        "drupal/taxonomy_menu": "^3.3",
        "drupal/term_reference_tree": "^1.0",
        "drupal/token": ">= 1.0",
        "drupal/transliterate_filenames": "^1.3",
        "drupal/twig_field_value": ">= 1.1",
        "drupal/twig_tweak": ">= 2.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_slideshow": "^4.6",
        "drupal/webform": "^6.0",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "drupal/core-dev": "^9.1",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
